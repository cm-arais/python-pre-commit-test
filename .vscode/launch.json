    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: pre-commit",
            "type": "python",
            "request": "launch",
            "pythonPath": "${config:python.pythonPath}",
            "program": ".venv/bin/pre-commit",
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "console": "integratedTerminal",
            // "internalConsoleOptions": "openOnSessionStart",
            "jsutMyCode": false, //Debugモードの時にライブラリにブレイクポイントおける,
            "debugStdLib": true
        },
]
}